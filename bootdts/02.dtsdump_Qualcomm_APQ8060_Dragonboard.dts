/dts-v1/;

/ {
	#address-cells = < 0x01 >;
	#size-cells = < 0x01 >;
	model = "Qualcomm APQ8060 Dragonboard";
	compatible = "qcom,apq8060-dragonboard\0qcom,msm8660";
	interrupt-parent = < 0x01 >;

	chosen {
		stdout-path = "serial0:115200n8";
	};

	aliases {
		serial0 = "/soc/gsbi@19c00000/serial@19c40000";
	};

	memory {
		device_type = "memory";
		reg = < 0x00 0x00 >;
	};

	cpus {
		#address-cells = < 0x01 >;
		#size-cells = < 0x00 >;

		cpu@0 {
			compatible = "qcom,scorpion";
			enable-method = "qcom,gcc-msm8660";
			device_type = "cpu";
			reg = < 0x00 >;
			next-level-cache = < 0x02 >;
		};

		cpu@1 {
			compatible = "qcom,scorpion";
			enable-method = "qcom,gcc-msm8660";
			device_type = "cpu";
			reg = < 0x01 >;
			next-level-cache = < 0x02 >;
		};

		l2-cache {
			compatible = "cache";
			cache-level = < 0x02 >;
			phandle = < 0x02 >;
		};
	};

	cpu-pmu {
		compatible = "qcom,scorpion-mp-pmu";
		interrupts = < 0x01 0x09 0x304 >;
	};

	clocks {

		cxo_board {
			compatible = "fixed-clock";
			#clock-cells = < 0x00 >;
			clock-frequency = < 0x124f800 >;
		};

		pxo_board {
			compatible = "fixed-clock";
			#clock-cells = < 0x00 >;
			clock-frequency = < 0x19bfcc0 >;
		};

		sleep_clk {
			compatible = "fixed-clock";
			#clock-cells = < 0x00 >;
			clock-frequency = < 0x8000 >;
		};
	};

	soc {
		#address-cells = < 0x01 >;
		#size-cells = < 0x01 >;
		ranges;
		compatible = "simple-bus";
		phandle = < 0x1d >;

		interrupt-controller@2080000 {
			compatible = "qcom,msm-8660-qgic";
			interrupt-controller;
			#interrupt-cells = < 0x03 >;
			reg = < 0x2080000 0x1000 0x2081000 0x1000 >;
			phandle = < 0x01 >;
		};

		timer@2000000 {
			compatible = "qcom,scss-timer\0qcom,msm-timer";
			interrupts = < 0x01 0x00 0x301 0x01 0x01 0x301 0x01 0x02 0x301 >;
			reg = < 0x2000000 0x100 >;
			clock-frequency = < 0x19bfcc0 0x8000 >;
			cpu-offset = < 0x40000 >;
		};

		pinctrl@800000 {
			compatible = "qcom,msm8660-pinctrl";
			reg = < 0x800000 0x4000 >;
			gpio-controller;
			#gpio-cells = < 0x02 >;
			interrupts = < 0x00 0x10 0x04 >;
			interrupt-controller;
			#interrupt-cells = < 0x02 >;
			phandle = < 0x0b >;

			sdcc1 {
				phandle = < 0x15 >;

				mux {
					pins = "gpio159\0gpio160\0gpio161\0gpio162\0gpio163\0gpio164\0gpio165\0gpio166\0gpio167\0gpio168";
					function = "sdc1";
				};

				clk {
					pins = "gpio167";
					drive-strength = < 0x10 >;
					bias-disable;
				};

				cmd {
					pins = "gpio168";
					drive-strength = < 0x0a >;
					bias-pull-up;
				};

				data {
					pins = "gpio159\0gpio160\0gpio161\0gpio162\0gpio163\0gpio164\0gpio165\0gpio166";
					drive-strength = < 0x0a >;
					bias-pull-up;
				};
			};

			sdcc3 {
				phandle = < 0x18 >;

				clk {
					pins = "sdc3_clk";
					drive-strength = < 0x08 >;
					bias-disable;
				};

				cmd {
					pins = "sdc3_cmd";
					drive-strength = < 0x08 >;
					bias-pull-up;
				};

				data {
					pins = "sdc3_data";
					drive-strength = < 0x08 >;
					bias-pull-up;
				};
			};

			sdcc5 {
				phandle = < 0x1a >;

				mux {
					pins = "gpio95\0gpio96\0gpio97\0gpio98\0gpio99\0gpio100";
					function = "sdc5";
				};

				clk {
					pins = "gpio97";
					drive-strength = < 0x10 >;
					bias-disable;
				};

				cmd {
					pins = "gpio95";
					drive-strength = < 0x0a >;
					bias-pull-up;
				};

				data {
					pins = "gpio96\0gpio98\0gpio99\0gpio100";
					drive-strength = < 0x0a >;
					bias-pull-up;
				};
			};

			gsbi12_i2c {
				phandle = < 0x06 >;

				mux {
					pins = "gpio115\0gpio116";
					function = "gsbi12";
				};

				pinconf {
					pins = "gpio115\0gpio116";
					drive-strength = < 0x10 >;
					bias-disable;
				};
			};

			gsbi12_serial {
				phandle = < 0x05 >;

				mux {
					pins = "gpio117\0gpio118";
					function = "gsbi12";
				};

				tx {
					pins = "gpio117";
					drive-strength = < 0x08 >;
					bias-disable;
				};

				rx {
					pins = "gpio118";
					drive-strength = < 0x02 >;
					bias-pull-up;
				};
			};
		};

		clock-controller@900000 {
			compatible = "qcom,gcc-msm8660";
			#clock-cells = < 0x01 >;
			#reset-cells = < 0x01 >;
			reg = < 0x900000 0x4000 >;
			phandle = < 0x03 >;
		};

		gsbi@19c00000 {
			compatible = "qcom,gsbi-v1.0.0";
			cell-index = < 0x0c >;
			reg = < 0x19c00000 0x100 >;
			clocks = < 0x03 0x88 >;
			clock-names = "iface";
			#address-cells = < 0x01 >;
			#size-cells = < 0x01 >;
			ranges;
			syscon-tcsr = < 0x04 >;
			status = "ok";
			qcom,mode = < 0x06 >;
			phandle = < 0x1e >;

			serial@19c40000 {
				compatible = "qcom,msm-uartdm-v1.3\0qcom,msm-uartdm";
				reg = < 0x19c40000 0x1000 0x19c00000 0x1000 >;
				interrupts = < 0x00 0xc3 0x00 >;
				clocks = < 0x03 0xa0 0x03 0x88 >;
				clock-names = "core\0iface";
				status = "ok";
				pinctrl-names = "default";
				pinctrl-0 = < 0x05 >;
				phandle = < 0x1f >;
			};

			i2c@19c80000 {
				compatible = "qcom,i2c-qup-v1.1.1";
				reg = < 0x19c80000 0x1000 >;
				interrupts = < 0x00 0xc4 0x00 >;
				clocks = < 0x03 0xb8 0x03 0x88 >;
				clock-names = "core\0iface";
				#address-cells = < 0x01 >;
				#size-cells = < 0x00 >;
				status = "ok";
				pinctrl-names = "default";
				pinctrl-0 = < 0x06 >;
				phandle = < 0x20 >;

				ak8975@0c {
					compatible = "asahi-kasei,ak8975";
					reg = < 0x0c >;
					interrupt-parent = < 0x07 >;
					interrupts = < 0xe0 0x01 >;
					pinctrl-names = "default";
					pinctrl-0 = < 0x08 >;
					vid-supply = < 0x09 >;
					vdd-supply = < 0x0a >;
				};

				bmp085@77 {
					compatible = "bosch,bmp085";
					reg = < 0x77 >;
					interrupt-parent = < 0x07 >;
					interrupts = < 0xcf 0x01 >;
					reset-gpios = < 0x0b 0x56 0x01 >;
					pinctrl-names = "default";
					pinctrl-0 = < 0x0c >;
					vddd-supply = < 0x09 >;
					vdda-supply = < 0x0a >;
				};
			};
		};

		qcom,ssbi@500000 {
			compatible = "qcom,ssbi";
			reg = < 0x500000 0x1000 >;
			qcom,controller-type = "pmic-arbiter";

			pmic@0 {
				compatible = "qcom,pm8058";
				interrupt-parent = < 0x0b >;
				interrupts = < 0x58 0x08 >;
				#interrupt-cells = < 0x02 >;
				interrupt-controller;
				#address-cells = < 0x01 >;
				#size-cells = < 0x00 >;
				phandle = < 0x0d >;

				gpio@150 {
					compatible = "qcom,pm8058-gpio\0qcom,ssbi-gpio";
					reg = < 0x150 >;
					interrupt-parent = < 0x0d >;
					interrupts = < 0xc0 0x00 0xc1 0x00 0xc2 0x00 0xc3 0x00 0xc4 0x00 0xc5 0x00 0xc6 0x00 0xc7 0x00 0xc8 0x00 0xc9 0x00 0xca 0x00 0xcb 0x00 0xcc 0x00 0xcd 0x00 0xce 0x00 0xcf 0x00 0xd0 0x00 0xd1 0x00 0xd2 0x00 0xd3 0x00 0xd4 0x00 0xd5 0x00 0xd6 0x00 0xd7 0x00 0xd8 0x00 0xd9 0x00 0xda 0x00 0xdb 0x00 0xdc 0x00 0xdd 0x00 0xde 0x00 0xdf 0x00 0xe0 0x00 0xe1 0x00 0xe2 0x00 0xe3 0x00 0xe4 0x00 0xe5 0x00 0xe6 0x00 0xe7 0x00 0xe8 0x00 0xe9 0x00 0xea 0x00 0xeb 0x00 >;
