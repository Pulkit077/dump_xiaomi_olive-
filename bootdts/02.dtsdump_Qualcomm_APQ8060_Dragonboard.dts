							pins = "gpio16";
							function = "normal";
							input-enable;
							bias-disable;
							power-source = < 0x02 >;
						};
					};

					sdcc3-gpios {
						phandle = < 0x19 >;

						pinconf {
							pins = "gpio22";
							function = "normal";
							input-enable;
							bias-disable;
							power-source = < 0x02 >;
						};
					};

					sdcc5-gpios {
